{
  "[dockerfile]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[go.mod]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "[html]": {
    "editor.formatOnSave": false
  },
  "[ignore]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[lua]": {
    "editor.defaultFormatter": "sumneko.lua"
  },
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 200
  },
  "[nginx]": {
    "editor.defaultFormatter": "raynigon.nginx-formatter"
  },
  "[plaintext]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "other": "off",
      "strings": "off"
    }
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "[python]": {
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnSave": false
  },
  "[twig]": {
    "editor.formatOnSave": false
  },
  "breadcrumbs.enabled": true,
  "code-runner.enableAppInsights": false,
  "cSpell.enabled": true,
  "cSpell.language": "en,ru",
  "cSpell.userWords": [
    "ansible",
    "iryston",
    "metafile",
    "outdir",
    "tailwindcss"
  ],
  "css.lint.duplicateProperties": "warning",
  "css.lint.idSelector": "error",
  "css.lint.zeroUnits": "error",
  "css.validate": false,
  "dart.allowAnalytics": false,
  "dart.analysisServerFolding": false,
  "dart.analyzeAngularTemplates": false,
  "dart.flutterHotReloadOnSave": "never",
  "diffEditor.renderSideBySide": true,
  "dotenv.enableAutocloaking": false,
  "editor.accessibilitySupport": "off",
  "editor.autoIndent": "none",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorWidth": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.dragAndDrop": false,
  "editor.fontFamily": "'SFMono Nerd Font Mono', 'JetBrainsMonoNL Nerd Font Mono', 'MesloLGS NF', 'IosevkaTerm Nerd Font Mono', 'Hack Nerd Font Mono', 'JetBrains Mono', 'Roboto Mono', 'Input Mono', Hack, 'Droid Sans Mono', Menlo, Monaco, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": false,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.formatOnType": true,
  "editor.insertSpaces": false,
  "editor.lightbulb.enabled": false,
  "editor.lineHeight": 1.6,
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": false,
  "editor.quickSuggestions": {
    "comments": false,
    "other": true,
    "strings": true
  },
  "editor.quickSuggestionsDelay": 100,
  "editor.renderControlCharacters": true,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 120, 200],
  "editor.selectionHighlight": true,
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "comments": "#565d68",
    "textMateRules": []
  },
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "postcss": "css",
    "twig": "twig",
    "vue": "html",
    "vue-html": "html"
  },
  "emmet.triggerExpansionOnTab": true,
  "eslint.probe": [
    "html",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "eslint.run": "onSave",
  "eslint.validate": [
    "html",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "vue"
  ],
  "extensions.autoCheckUpdates": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "**/mysql/**/*.cnf": "ini",
    "*.inc": "php",
    "*.install": "php",
    "*.module": "php",
    "*.profile": "php",
    "*.theme": "php",
    ".env*": "dotenv",
    "Vagrantfile": "ruby"
  },
  "files.exclude": {
    "**/.DS_Store": true,
    "**/__pycache__": true
  },
  "files.hotExit": "off",
  "files.restoreUndoStack": false,
  "githubPullRequests.telemetry.enabled": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false
  },
  "gitlens.advanced.telemetry.enabled": false,
  "gitlens.blame.avatars": false,
  "gitlens.hovers.avatars": false,
  "gitlens.hovers.detailsMarkdownFormat": "[__${author}__](mailto:${email}), ${ago} &nbsp; _(${date})_ ${message}\n\n${commands}",
  "gitlens.keymap": "none",
  "gitlens.plusFeatures.enabled": false,
  "gitlens.showWhatsNewAfterUpgrades": false,
  "gitlens.telemetry.enabled": false,
  "gitlens.views.branches.avatars": false,
  "gitlens.views.commits.avatars": false,
  "gitlens.views.contributors.avatars": false,
  "gitlens.views.fileHistory.avatars": false,
  "gitlens.views.lineHistory.avatars": false,
  "gitlens.views.remotes.avatars": false,
  "gitlens.views.repositories.avatars": false,
  "gitlens.views.searchAndCompare.avatars": false,
  "gitlens.views.tags.avatars": false,
  "go.toolsManagement.autoUpdate": true,
  "go.useLanguageServer": true,
  "html.format.indentInnerHtml": true,
  "html.format.maxPreserveNewLines": 1,
  "html.format.wrapLineLength": 0,
  "indentRainbow.indicatorStyle": "light",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.maxItemsComputed": 10000,
  "less.lint.duplicateProperties": "warning",
  "less.lint.idSelector": "error",
  "less.lint.zeroUnits": "error",
  "less.validate": false,
  "Lua.telemetry.enable": false,
  "markdown.preview.scrollEditorWithPreview": false,
  "markdown.preview.scrollPreviewWithEditor": false,
  "notebook.outputLineHeight": 20,
  "partialDiff.enableTelemetry": false,
  "PT Application Inspector settings.Allow telemetry collection": false,
  "PT Application Inspector settings.Automatically enable for any project": false,
  "python.experiments.enabled": false,
  "python.languageServer": "Default",
  "redhat.telemetry.enabled": false,
  "rest-client.enableTelemetry": false,
  "scss.lint.duplicateProperties": "warning",
  "scss.lint.idSelector": "error",
  "scss.lint.zeroUnits": "error",
  "scss.validate": false,
  "search.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/__pycache__": true,
    "**/bower_components": true,
    "**/CVS": true,
    "**/node_modules": true
  },
  "search.smartCase": true,
  "search.useIgnoreFiles": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.emptyWindow": false,
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.untrustedFiles": "open",
  "shellcheck.ignorePatterns": {
    "**/*.local": true
  },
  "stylelint.enable": true,
  "svelte.enable-ts-plugin": true,
  "svg.preview.mode": "svg",
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.env.linux": {
    "GTK_PATH": null
  },
  "textmarker.enableTelemetry": false,
  "todo-tree.general.tags": [
    "[ ]",
    "[x]",
    "BUG",
    "FIXME",
    "HACK",
    "TODO",
    "XXX"
  ],
  "todo-tree.highlights.enabled": true,
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "typescript.disableAutomaticTypeAcquisition": true,
  "update.mode": "none",
  "update.showReleaseNotes": false,
  "window.autoDetectColorScheme": true,
  "window.density.editorTabHeight": "compact",
  "window.restoreWindows": "none",
  "window.title": "${activeEditorLong}${separator}${rootName}",
  "window.zoomLevel": 1.5,
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#b58800",
    "tab.unfocusedActiveBorder": "#b58800",
    "[Default Dark Modern]": {
      "activityBarBadge.background": "#405275",
      "activityBarBadge.foreground": "#eceff4",
      "button.background": "#405275",
      "button.foreground": "#eceff4",
      "button.hoverBackground": "#2c3d5c"
    }
  },
  "workbench.colorTheme": "Default Dark Modern",
  "workbench.editor.empty.hint": "hidden",
  "workbench.editor.untitled.labelFormat": "name",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.enableExperiments": false,
  "workbench.fontAliasing": "auto",
  "workbench.preferredDarkColorTheme": "Default Dark Modern",
  "workbench.preferredLightColorTheme": "Quiet Light",
  "workbench.settings.applyToAllProfiles": [
    "editor.bracketPairColorization.enabled",
    "editor.bracketPairColorization.independentColorPoolPerBracketType"
  ],
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.settings.useSplitJSON": true,
  "workbench.startupEditor": "newUntitledFile",
  "zenMode.centerLayout": true,
  "zenMode.fullScreen": true,
  "zenMode.hideLineNumbers": false
}
